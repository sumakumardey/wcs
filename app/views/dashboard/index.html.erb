<script>
$(".nlq-form").submit(function(event) {
  query = $(".nlq-query").val();
  $.ajax({
    dataType: "json",
    url: "/query.json",
    data: {text: query},
    success: function(data) {
      var tbl_body = "";
      $.each(data, function() {
        tbl_row = "<tr>";
        tbl_row += "<td>" + this.waypoint_date + "</td>"
        tbl_row += "<td>" + this.patrol_id + "</td>"
        tbl_row += "<td>" + this.patrol_leg_id + "</td>"
        tbl_row += "<td>" + this.leader + "</td>"
        tbl_row += "<td>" + this.species + "</td>"
        tbl_row += "<td>" + this.observation_category_0 + " " + this.observation_category_1 + "</td>"
        tbl_row += "</tr>";
        tbl_body += tbl_row;
      });
      $(".nlq-results-table tbody").html(tbl_body);
      $(".nlq-count").text("(" + data.length + ")");
      console.log("Response Data",data);
    }
  });
  event.preventDefault();
});


$("#sparkline1").sparkline([1,10,1,1,1,10,4,5,2,6,2], {
